def make_number(x, y = 0):
    """doc ctring(함수의 도움말)
    매개변수 2개를 이용해서 2자리 정수를 만들어요
    x = 10의 자리수
    y = 1의 자리수 (default값 0)
    """
    return x * 10 + y


make_number(3, 2)


make_number(5) # y입력안하면 0


make = make_number # 함수를 변수에 할당


make(6)





del make


