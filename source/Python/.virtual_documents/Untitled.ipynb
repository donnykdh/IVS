import random
num = random.randint(1, 100)
print("정답은 : {}".format(num))
min = 1
max = 100
chance = 5
while 1:
    answer = int(input(str(min) + '~' + str(max) + "숫자를 입력하세요 (남은 기회는 " + str(chance) + "번 남았습니다) >> "))
    if (answer < min) | (answer > max):
        print ("수의 범위를 벗어났습니다")
        chance -= 1
    elif answer > num:
        print("Down")
        max = answer - 1
        chance -= 1
    elif answer < num:
        print("Up")
        min = answer + 1
        chance -= 1
    else:
        print("정답입니다")
        break
    if chance == 0:
        print("5번의 기회가 끝났습니다")
        break





